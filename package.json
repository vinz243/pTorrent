{
  "name": "pTorrent",
  "private": false,
  "version": "0.0.2",
  "description": "Fast and modern material design torrent client ",
  "main": "main.js",
  "scripts": {
    "start": "node server.js --env=dev",
    "test": "karma start",
    "test:watch": "karma start --autoWatch=true --singleRun=false",
    "posttest": "npm run lint",
    "serve": "node server.js --env=dev",
    "gui": "node server.js --env=gui",
    "serve:dist": "node server.js --env=dist",
    "dist": "npm run copy & webpack --env=dist",
    "lint": "eslint ./src",
    "copy": "copyfiles -f ./src/index.html ./src/favicon.ico ./dist",
    "clean": "rimraf dist/*",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:patch": "npm version patch && npm publish && git push --follow-tags",
    "install": "browserify -s WebTorrent -e node_modules/webtorrent/ > node_modules/webtorrent/webtorrent.debug.js"
  },
  "repository": "",
  "keywords": [],
  "author": "vinz243",
  "devDependencies": {
    "babel-core": "^6.3.26",
    "babel-eslint": "^5.0.0-beta4",
    "babel-loader": "^6.0.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "bower-webpack-plugin": "^0.1.9",
    "browserify": "*",
    "chai": "^3.2.0",
    "copyfiles": "^0.2.1",
    "css-loader": "^0.23.0",
    "es6-promise": "^3.0.2",
    "eslint": "^1.2.1",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-react": "^3.3.0",
    "glob": "^6.0.0",
    "isparta-instrumenter-loader": "^1.0.0",
    "karma": "^0.13.9",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^0.5.3",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.1.1",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-phantomjs-shim": "^1.1.1",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.7.0",
    "minimist": "^1.2.0",
    "mocha": "^2.2.5",
    "node-sass": "^3.4.2",
    "null-loader": "^0.1.1",
    "open": "0.0.5",
    "phantomjs": "*",
    "postcss": "^5.0.11",
    "postcss-loader": "^0.8.0",
    "react-hot-loader": "^1.2.9",
    "rimraf": "^2.4.3",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.0",
    "webpack-dev-server": "^1.12.0"
  },
  "dependencies": {
    "classnames": "^2.2.1",
    "core-js": "^2.0.0",
    "debug": "^2.2.0",
    "fluxxor": "^1.7.3",
    "humanize": "0.0.9",
    "lodash": "^3.10.1",
    "material-ui": "^0.14.0-rc2",
    "normalize.css": "^3.0.3",
    "parse-torrent": "^5.7.0",
    "pretty-bytes": "*",
    "pretty-error": "^2.0.0",
    "react": "^0.14.0",
    "react-addons-test-utils": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-tap-event-plugin": "^0.2.1",
    "webpack-target-electron-renderer": "^0.3.0",
    "webtorrent": "^0.63.4",
    "xmlrpc": "^1.3.1"
  }
}
